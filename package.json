{
  "name": "avett-rx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p types:watch next:dev",
    "next:dev": "next dev",
    "dev:e2e": "run-s types:build next:e2e",
    "next:e2e": "MOCK_SERVICE_WORKER=1 next dev",
    "dev:ci": "MOCK_SERVICE_WORKER=1 next dev",
    "prebuild": "CI=true yarn test",
    "build": "next build",
    "start": "next start",
    "types:build": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen --config codegen.yml -r dotenv/config",
    "types:watch": "yarn types:build --watch",
    "lint": "eslint --ext .ts,.tsx src/",
    "format": "prettier --write \"src/**/*\"",
    "schema:update": "./scripts/update-schema.sh",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o public/storybook",
    "pretest": "yarn types:build",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn jest --coverage",
    "test:watch:coverage": "yarn jest --coverage --watch",
    "cy:open": "yarn cypress open",
    "cy:run": "yarn cypress run",
    "test:cy-open": "start-server-and-test dev:e2e http://localhost:3000 cy:open",
    "test:cy-run": "start-server-and-test dev:ci http://localhost:3000 cy:run",
    "pretest:ci": "yarn types:build",
    "test:ci": "CI=true yarn test && yarn test:cy-run"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@hapi/iron": "^6.0.0",
    "@magic-sdk/admin": "^1.3.0",
    "@reach/dialog": "^0.11.2",
    "@reach/visually-hidden": "^0.12.0",
    "cookie": "^0.4.1",
    "faunadb": "^4.0.0",
    "graphql": "^15.4.0",
    "magic-sdk": "^4.0.0",
    "next": "10.0.3",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/fragment-matcher": "2.0.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@storybook/addon-actions": "^6.1.1",
    "@storybook/addon-essentials": "^6.1.8",
    "@storybook/addon-links": "^6.1.8",
    "@storybook/react": "^6.1.7",
    "@testing-library/cypress": "^7.0.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "@tmikeschu/graphql-typescript-factories": "^1.0.5",
    "@tmikeschu/graphql-typescript-response-factories": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "cross-fetch": "^3.0.6",
    "cypress": "^5.6.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest-dom": "^3.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": ">=4",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "msw": "^0.22.3",
    "npm-run-all": "^4.1.5",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.0",
    "react-is": "^17.0.1",
    "start-server-and-test": "^1.11.6",
    "tailwindcss": "^1.9.6",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "vercel": "^21.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{css,md}": "prettier --write"
  }
}
